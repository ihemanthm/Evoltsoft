{
  "headers": [
    {
      "source" : "**/*.@(css|woff|woff2|ttf|eot|svg)",
      "headers" : [ {
        "key" : "Content-Type",
        "value" : "text/css; charset=utf-8"
      } ]
    },
    {
      "source" : "**/*.@(js|jsx|ts|tsx)",
      "headers" : [ {
        "key" : "Content-Type",
        "value" : "application/javascript; charset=utf-8"
      } ]
    },
    {
      "source" : "**/*.@(json)",
      "headers" : [ {
        "key" : "Content-Type",
        "value" : "application/json"
      } ]
    },
    {
      "source" : "**/*.@(jpg|jpeg|png|gif|ico|webp)",
      "headers" : [ {
        "key" : "Content-Type",
        "value" : "image/webp"
      } ]
    }
  ]
}
